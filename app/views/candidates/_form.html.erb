<ul><%= form_for @candidate do |f| %></ul>
<ul><%= f.label :first_name%></ul>
<ul><%= f.text_field :first_name%></ul>
<ul><%= f.label :last_name%></ul>
<ul><%= f.text_field :last_name%></ul>
<ul><%= f.label :jurisdiction%></ul>
<ul><%= f.text_field :jurisdiction%></ul>
<ul><%= f.label :party%></ul>
<ul><%= f.text_field :party%></ul>
<ul><%= f.label :photo_url, style: 'display:none' %></ul>
<ul><%= f.text_field :photo_url, style: 'display:none' %></ul>
<ul><%= f.label :bioguide_id, style: 'display:none' %></ul>
<ul><%= f.text_field :bioguide_id, style: 'display:none' %></ul>
<ul><%= f.label :incumbent %></ul>
<ul><%= f.check_box :incumbent%></ul>
<ul><%= f.submit %></ul>
<% end %>
